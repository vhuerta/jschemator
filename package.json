{
  "name": "jschemator",
  "version": "0.1.4",
  "description": "Simple npm package to validate an object using JSON Schema, and get errors in friendly form, useful to validate on frontend with VUE or maybe REACT!",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_ENV=test ./node_modules/.bin/mocha -b",
    "clean": "rm -rf ./*.js",
    "build": "./node_modules/.bin/babel src -d .",
    "prepublish": "npm run clean && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vhuerta/jschemator.git"
  },
  "keywords": [
    "json",
    "schema",
    "validator",
    "i18n",
    "vue"
  ],
  "author": "Victor Huerta <vhuertahnz@gmail.com>",
  "contributors": [
    "Victor Huerta <vhuertahnz@gmail.com>"
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/vhuerta/jschemator/issues"
  },
  "homepage": "https://github.com/vhuerta/jschemator#readme",
  "dependencies": {
    "ajv": "^4.10.4",
    "ajv-i18n": "^1.3.0",
    "babel-preset-es2015": "^6.24.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-preset-node5": "^12.0.0",
    "babel-preset-stage-2": "^6.24.1",
    "mocha": "^3.2.0",
    "should": "^11.1.2"
  }
}
